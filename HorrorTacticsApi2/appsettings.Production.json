{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "JwtGenerator": {
    "SigningKey": "R"
  },
  "General": {
    "MainPassword": "R",
    "UploadPath": "R"
  },
  "ConnectionStrings": {
    "Main": "Redacted"
  },
  "SentryOptions": {
    "Dsn": "R",
    "Enable": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  // More info: https://docs.microsoft.com/en-us/aspnet/core/fundamentals/servers/kestrel?view=aspnetcore-3.1#listenoptionsusehttps
  "Kestrel": {
    "Endpoints": {
      "HTTP": {
        "Url": "http://localhost:5008"
      }
      // Not needed for prod env
      //"HTTPS": {
      //  "Url": "https://localhost:5001",
      //  "Certificate": {
      //    "Path": "/etc/apache2/ssl/self-cert.pfx",
      //    "Password": ""
      //  }
      //}
    }
  }
}
